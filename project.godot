config_version=5

[application]

config/name="Boomer"
run/main_scene="res://scenes/game.tscn"
config/features=PackedStringArray("4.2", "C#", "Mobile")
run/max_fps=60
config/icon="res://icon.svg"

[autoload]

SignalBus="*res://autoloads/signal_bus.gd"
BeatManager="*res://autoloads/beat_manager.gd"

[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/stretch/mode="viewport"
window/stretch/aspect="expand"
mouse_cursor/custom_image="res://assets/themes/Crosshair15.png"

[dotnet]

project/assembly_name="Boomer"

[editor_plugins]

enabled=PackedStringArray("res://addons/qodot/plugin.cfg")

[file_customization]

folder_colors={
